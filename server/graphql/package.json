{
  "name": "relay-fullstack",
  "version": "1.0.0",
  "description": "Relay Starter Kit - Integrated with Relay, GraphQL, Express, ES6/ES7, JSX, Webpack, Babel, Material Design Lite, and PostCSS",
  "scripts": {
    "start": "nodemon --watch \"./data/**/*.js\" --exec \"yarn update && yarn start-sub-task\"",
    "start-sub-task": "./node_modules/.bin/babel-node ./index.js",
    "update": "./node_modules/.bin/babel-node ./utils/updateSchema.js"
  },
  "devDependencies": {
    "babel-jest": "^22.0.4",
    "babel-plugin-relay": "dev",
    "babel-polyfill": "^6.26.0",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-relay-plugin": "^0.11.0",
    "chalk": "^2.3.0",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.3",
    "hapi": "^18.1.0",
    "lodash": "^4.17.4",
    "lowdb": "^1.0.0",
    "moment": "^2.20.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.9.4"
  }
}
